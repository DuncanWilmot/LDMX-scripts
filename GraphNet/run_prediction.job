#!/bin/bash
#SBATCH -N 1 --partition=gpu --ntasks-per-node=1
#SBATCH --gres=gpu:1

export MODEL_NAME=v12_model_name
export DATA_NAME=v12_data_name
export BASE_DIRECTORY=/home/pmasterson/ParticleNet/LDMX-scripts/GraphNet

cd $SLURM_SUBMIT_DIR

